# Cloud Run Deployment Environment Variables
# Copy this file to .env and fill in your values

# Google Cloud Project Configuration
GCP_PROJECT_ID=xzerolab-480008
GCP_REGION=asia-northeast1

# GCS Bucket for persistent storage
GCS_BUCKET_NAME=openclawbot-data

# Service Account (optional, will be auto-generated if not set)
SERVICE_ACCOUNT_EMAIL=openclawbot-sa@xzerolab-480008.iam.gserviceaccount.com

# Gateway Authentication Token
# Generate with: openssl rand -base64 32
# Default: mNrXA9Lm+5cs6wMziYMafJgkjTJg45OMiB1YTXEt5E8=
OPENCLAW_GATEWAY_TOKEN=mNrXA9Lm+5cs6wMziYMafJgkjTJg45OMiB1YTXEt5E8=

# Alternative: Use INTERNAL_SERVICE_TOKEN format (compatible)
# OPENCLAW_GATEWAY_TOKEN=uTvryFvAbz6M5sRtmTaSTQY6otLZ95hneBsWqXu+35I=
